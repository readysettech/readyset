# 2. Newly created tables should get seen by existing queries

skipif readyset
statement ok
CREATE SCHEMA s1

skipif readyset
statement ok
CREATE SCHEMA s2

statement ok
create table s2.t (x int);

statement ok
insert into s2.t (x) values (2);

statement ok
set search_path = s1, s2;

query I nosort
select x from t;
----
2

statement ok
create table s1.t (x int);

statement ok
insert into s1.t (x) values (1);


graphviz

query I nosort
select x from t;
