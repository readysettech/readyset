3
4
5
6


# with manually-desugared BETWEEN
query I rowsort
select x from t1 where x >= ? and x <= ?
? = 3
? = 6
----
3
4
5
6

# with manually-desugared BETWEEN, reversed
query I rowsort
select x from t1 where x <= ? and x >= ?
? = 6
? = 3
----
3
4
5
