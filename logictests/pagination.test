# Paginate post evicty-join

statement ok
create table t2 (grp2 int, val int);

statement ok
create cache from
select t1.val
from t1
join t2 on t1.val = t2.val
where grp2 = ?
order by val asc
limit 3
offset ?;

statement ok
insert into t2 (grp2, val) values (1, 1), (1, 2);

query I nosort
select t1.val
from t1
join t2 on t1.val = t2.val
where grp2 = ?
order by val asc
limit 3
offset ?
? = 1
? = 0
